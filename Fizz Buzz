//fizzBuzz compara valores e tem um tipo de retorno baseado no valor de entrada

//Divisível por 3 => Fizz
//Divisível por 5 => Buzz
////Divisível por 3 e 5 => fizzBuzz
//Não divisível por 3 ou 5 => retorna o número
//Não sendo número retorna "não é um número"
const resultado = fizzBuzz(2);
console.log( resultado);

function fizzBuzz(entrada){
   if (typeof entrada !== 'number' )
      return ' Não é um número';
   if (entrada % 3 === 0 && entrada % 5 === 0)
      return 'Fizz Buzz';
   if (entrada % 3 === 0)
      return 'Fizz';
   if (entrada % 5 === 0)
      return 'Buzz';
   
   return entrada;

}
