// escreva um afunção que usa 2 números e retorna o maior entre eles


let valorMaior = max(10,120);
console.log(valorMaior);
function max(numero1,numero2){
   if(numero1 > numero2)
      return numero1;
    return numero2;

}

//segunda forma 


let valorMaior = max(10,120);
console.log(valorMaior);

function max(numero1,numero2){
   return numero1 > numero2 ? numero1: numero2;
}
