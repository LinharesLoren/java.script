//natureza dinâmica de objetos, só para entender que são dinãmicos e podem ser mutáveis
const mouse = {
   cor : 'red',
   marcar : 'dazz'
}
mouse.velocidade = 5000;
mouse.trocarDPI = function (){
   console.log('mudando DPI');

}
delete mouse.velocidade;
delete mouse.trocarDpi;
console.log(mouse);
